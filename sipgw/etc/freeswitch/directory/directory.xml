<domain name="$${jigate_domain}">
    <groups>
        <group name="200">
            <users>
                <user id="user">
                    <params>
                        <param name="password" value="$${user_password}"/>
                        <param name="dial-string" value="${sofia_contact(${dialed_user}@${dialed_domain})}"/>
                    </params>
                    <variables>
                        <variable name="sip-force-contact" value="NDLB-connectile-dysfunction-2.0"/>
                    </variables>
                </user>
            </users>
        </group>
        <group name="300">
            <users>
                <user id="$${jigate_username}">
                    <params>
                        <param name="password" value="$${jigate_password}"/>
                        <param name="dial-string" value="${sofia_contact(${dialed_user}@${dialed_domain})}"/>
                    </params>
                </user>
            </users>
        </group>
    </groups>
</domain>
