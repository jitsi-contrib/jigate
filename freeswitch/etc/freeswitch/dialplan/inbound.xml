<?xml version="1.0" encoding="utf-8"?>
<include>
   <context name="public">
      <extension name="inbound">
         <condition field="destination_number" expression=".*">
            <action application="event" data="Event-Name=INBOUND_CALL"/>
            <action application="park"/>
         </condition>
      </extension>
   </context>
</include>
